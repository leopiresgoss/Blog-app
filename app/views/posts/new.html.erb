<h1>New Post</h1>

<%= form_with url: user_posts_path(@current_user_id), method: :post, local: true, scope: :post do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div>
    <%= f.label :text %>
    <%= f.text_area :text %>
  </div>
  <%= f.submit "Submit" %>
<% end %>